<launch>
  <node name="so_dso_bow" pkg="so_dso_place_recognition" type="test_bow" output="screen" required="true">
    <param name="voc_file" value="$(find so_dso_place_recognition)/place_recognition/src/BoW/ORBvoc.txt" />
    <param name="incoming_id_file1" value="$(find so_dso_place_recognition)/place_recognition/results/RobotCar/$(arg date1)/incoming_id_file.txt" />
    <param name="incoming_id_file2" value="$(find so_dso_place_recognition)/place_recognition/results/RobotCar/$(arg date2)/incoming_id_file.txt" />
    <param name="bag1" value="/home/moxxx066/Workspace/data/bags/RobotCar/$(arg date1).bag" />
    <param name="bag2" value="/home/moxxx066/Workspace/data/bags/RobotCar/$(arg date2).bag" />
    <param name="img_topic" value="camera/left/image_raw" />
    <param name="output_file" value="$(find so_dso_place_recognition)/place_recognition/results/RobotCar/BoW/$(arg date1)_$(arg date2)/diff_mat.txt" />
  </node>
</launch>